<Window x:Class="PT_MAPACKET.ConnectFilesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Wybierz pliki" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>
        <Button Content="Połącz" Grid.Row="1" Click="Button_Click"/>
        <ListView x:Name="ListViewCFW" Grid.Row="0" SelectionMode="Multiple" Margin="0,0,0,31">
        </ListView>
        <CheckBox x:Name="deleteDuplicate" Content="Usuń duplikaty przy łączeniu" HorizontalAlignment="Left" Margin="10,194,0,0" VerticalAlignment="Top" Width="261" Height="21"/>
    </Grid>
</Window>
