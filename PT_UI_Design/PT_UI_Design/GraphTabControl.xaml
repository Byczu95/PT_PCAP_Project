<UserControl x:Class="PT_UI_Design.GraphTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:graphxctrl="clr-namespace:GraphX.Controls;assembly=GraphX"
             xmlns:models="clr-namespace:PT_UI_Design.Models"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PT_UI_Design"
             xmlns:controls="http://schemas.panthernet.ru/graphx/"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <controls:ZoomControl x:Name="zoomctrl" Grid.ColumnSpan="2" Margin="10,10,10,10">
            <models:GraphAreaPcap x:Name="Area"/>
        </controls:ZoomControl>
    </Grid>
</UserControl>
